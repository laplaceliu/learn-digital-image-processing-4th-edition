{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/external/**",
                "${workspaceFolder}/external/spdlog/include/**",
                "/usr/include/**",
                "/usr/local/include/**"
            ],
            "defines": [
                "__GNUC__=9",
                "__cplusplus=201703L"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "/usr/include/**",
                    "/usr/local/include/**"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "configurationProvider": "cppcheck"
        }
    ],
    "version": 4
}